{
  "name": "stand-bots",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "npm run build:ci",
    "now-build": "npm run build:ci",
    "build:ci": "npm run build:typescript && npm run db:migrate",
    "build:typescript": "tsc",
    "db:migrate": "sequelize db:migrate",
    "dev": "onchange \"src/**/*.ts\" -v -i -p -- node -r ts-node/register --inspect=127.0.0.1:9229 src/app.ts",
    "dev:pm2": "pm2 start ecosystem.dev.config.js --no-daemon",
    "start": "pm2 start ./build/app.js --no-daemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "8.9.0"
  },
  "dependencies": {
    "@types/mixpanel": "^2.14.0",
    "@types/node": "^9.6.5",
    "@types/node-fetch": "^1.6.8",
    "@types/node-json-db": "0.0.1",
    "@types/sequelize": "^4.27.14",
    "date-fns": "^2.0.0-alpha.7",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mixpanel": "^0.9.1",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.1",
    "pm2": "^2.10.2",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "sequelize-typescript": "^0.6.3",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1",
    "viber-bot": "^1.0.13"
  },
  "devDependencies": {
    "tslint": "^5.9.1",
    "onchange": "^3.3.0"
  }
