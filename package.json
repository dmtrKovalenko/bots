{
  "name": "stand-bots",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy": "node deploy-now.js",
    "now-local": "now --local-config=now.json",
    "deploy:stage:telegram": "npm run deploy -- --env stage --bot telegram && npm run now-local",
    "deploy:stage:viber": "npm run deploy -- --env stage --bot viber && npm run now-local",
    "deploy:stage": "npm run deploy:stage:telegram && npm run deploy:stage:viber",
    "build:typescript": "tsc",
    "tslint": "tslint src/**/*.ts",
    "db:migrate": "sequelize db:migrate",
    "dev:telegram": "onchange \"src/**/*.ts\" -v -i -p -- node -r ts-node/register --inspect=127.0.0.1:9229 src/bots/telegram.ts",
    "dev:viber": "onchange \"src/**/*.ts\" -v -i -p -- node -r ts-node/register --inspect=127.0.0.1:9229 src/bots/viber.ts",
    "dev": "onchange \"src/**/*.ts\" -v -i -p -- node -r ts-node/register --inspect=127.0.0.1:9229 src/app-dev.ts",
    "build": "npm run build:typescript",
    "start": "node build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "8.9.0"
  },
  "dependencies": {
    "@types/delay": "^2.0.1",
    "@types/luxon": "^0.5.2",
    "@types/mixpanel": "^2.14.0",
    "@types/node": "^9.6.7",
    "@types/node-fetch": "^1.6.9",
    "@types/node-json-db": "0.0.1",
    "@types/node-telegram-bot-api": "^0.28.2",
    "@types/sequelize": "^4.27.16",
    "commander": "^2.15.1",
    "delay": "^2.0.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "full-icu": "^1.2.1",
    "luxon": "^1.2.0",
    "mixpanel": "^0.9.1",
    "node-cmd": "^3.0.0",
    "node-fetch": "^2.1.2",
    "node-telegram-bot-api": "^0.30.0",
    "pg": "^7.4.1",
    "pm2": "^2.10.3",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "sequelize-typescript": "^0.6.4",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.3",
    "viber-bot": "^1.0.13"
  },
  "devDependencies": {
    "tslint": "^5.9.1",
    "onchange": "^3.3.0"
  }
}
