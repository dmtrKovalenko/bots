{
  "name": "stand-bots",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy": "node deploy-now.js",
    "now-local": "now --local-config=now.json",
    "deploy:stage:telegram": "npm run deploy -- --env stage --app telegram && npm run now-local",
    "deploy:stage:viber": "npm run deploy -- --env stage --app viber && npm run now-local",
    "deploy:stage:cron": "npm run deploy -- --env stage --app cron && npm run now-local",
    "deploy:stage": "npm run deploy:stage:telegram && npm run deploy:stage:viber && npm run deploy:stage:cron",
    "build:typescript": "tsc",
    "tslint": "npx tslint 'src/**/*.ts'",
    "db:migrate": "sequelize db:migrate",
    "dev": "TS_NODE_FILES=true onchange \"src/**/*.ts\" -v -i -p -- node -r ts-node/register --inspect=9229 src/app.ts",
    "build": "npm run build:typescript",
    "start": "node --icu-data-dir=./node_modules/full-icu build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "10.0.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.18",
    "@types/cache-manager": "^1.2.6",
    "@types/cron": "^1.3.0",
    "@types/delay": "^2.0.1",
    "@types/luxon": "^0.5.3",
    "@types/node": "^10.9.4",
    "@types/node-fetch": "^1.6.9",
    "@types/node-json-db": "0.0.1",
    "@types/node-telegram-bot-api": "^0.28.4",
    "@types/redis": "^2.8.6",
    "@types/sequelize": "^4.27.25",
    "@types/url-search-params": "^0.10.2",
    "bluebird": "^3.5.1",
    "cache-manager": "^2.9.0",
    "cache-manager-redis-store": "^1.5.0",
    "commander": "^2.17.1",
    "cron": "^1.4.1",
    "delay": "^2.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "full-icu": "^1.2.1",
    "luxon": "^1.3.3",
    "mixpanel": "^0.9.2",
    "node-cmd": "^3.0.0",
    "node-fetch": "^2.2.0",
    "node-telegram-bot-api": "^0.30.0",
