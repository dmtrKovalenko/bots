{
  "name": "stand-bots",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy": "node deploy-now.js",
    "now-local": "now --local-config=now.json",
    "deploy:stage:telegram": "npm run deploy -- --env stage --app telegram && npm run now-local",
    "deploy:stage:viber": "npm run deploy -- --env stage --app viber && npm run now-local",
    "deploy:stage:cron": "npm run deploy -- --env stage --app cron && npm run now-local",
    "deploy:stage": "npm run deploy:stage:telegram && npm run deploy:stage:viber && npm run deploy:stage:cron",
    "build:typescript": "tsc",
    "tslint": "tslint src/**/*.ts -c tslint.json -p tsconfig.json",
    "db:migrate": "sequelize db:migrate",
    "dev": "onchange \"src/**/*.ts\" -v -i -p -- node -r ts-node/register --inspect=9229 src/app.ts",
    "build": "npm run build:typescript",
    "start": "node --icu-data-dir=./node_modules/full-icu build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "10.0.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/cron": "^1.3.0",
    "@types/delay": "^2.0.1",
    "@types/luxon": "^0.5.2",
    "@types/node": "^10.0.0",
    "@types/node-fetch": "^1.6.9",
    "@types/node-json-db": "0.0.1",
    "@types/node-telegram-bot-api": "^0.28.3",
    "@types/sequelize": "^4.27.16",
    "commander": "^2.15.1",
    "cron": "^1.3.0",
    "delay": "^2.0.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "full-icu": "^1.2.1",
    "luxon": "^1.2.0",
    "mixpanel": "^0.9.1",
    "node-cmd": "^3.0.0",
    "node-fetch": "^2.1.2",
    "node-telegram-bot-api": "^0.30.0",
    "pg": "^7.4.1",
    "pm2": "^2.10.3",
    "reflect-metadata": "^0.1.12",
    "require-all": "^2.2.0",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "sequelize-typescript": "^0.6.4",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3",
    "viber-bot": "^1.0.13"
  },
  "devDependencies": {
    "tslint": "^5.9.1",
    "onchange": "^3.3.0"
  }
}
