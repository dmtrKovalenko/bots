{
  "name": "stand-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy": "node scripts/deploy-now.js",
    "now-local": "now --local-config=now.json",
    "deploy:stage:telegram": "npm run deploy -- --env stage --app telegram && npm run now-local",
    "deploy:stage:cron": "npm run deploy -- --env stage --app cron && npm run now-local",
    "deploy:stage": "npm run deploy:stage:telegram && npm run deploy:stage:cron",
    "build:typescript": "tsc",
    "tslint": "npx tslint 'src/**/*.ts'",
    "db:migrate": "sequelize db:migrate",
    "build": "npm run build:typescript",
    "start": "node --icu-data-dir=./node_modules/full-icu build/app.js",
    "dev": "TS_NODE_FILES=true onchange \"src/**/*.ts\" \"../bot-core/build/**/*.js\" -v -i -p -- node -r ts-node/register --inspect=9229 --icu-data-dir=./node_modules/full-icu src/app.ts",
    "prod": "pm2 reload ecosystem.config.js -a"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "10.0.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/cache-manager": "^1.2.6",
    "@types/cron": "^1.3.0",
    "@types/delay": "^3.1.0",
    "@types/luxon": "^1.2.2",
    "@types/node": "^10.9.4",
    "@types/node-fetch": "^2.1.2",
    "@types/node-json-db": "0.0.1",
    "@types/redis": "^2.8.6",
    "@types/sequelize": "^4.27.25",
    "@types/url-search-params": "^0.10.2",
    "bluebird": "^3.5.2",
    "cache-manager": "^2.9.0",
    "cache-manager-redis-store": "^1.5.0",
    "commander": "^2.18.0",
    "cron": "^1.4.1",
    "dotenv": "^6.0.0",
    "full-icu": "^1.2.1",
    "luxon": "^1.3.3",
    "mixpanel": "^0.9.2",
    "node-cmd": "^3.0.0",
    "node-fetch": "^2.2.0",
    "pg": "^7.4.3",
    "pm2": "^3.1.2",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "require-all": "^3.0.0",
    "sequelize": "^4.38.1",
    "sequelize-cli": "^4.1.1",
    "sequelize-typescript": "^0.6.6",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "url-search-params": "^1.1.0",
    "bot-core": "1.0.0"
  },
  "devDependencies": {
    "onchange": "^4.1.0",
    "tslint": "^5.11.0"
  }
}
